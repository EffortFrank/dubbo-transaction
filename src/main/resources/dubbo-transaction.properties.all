#是否开启事务代理
transaction.isProxy=true
#事务对象集合HASH映射数量
transaction.contextCount=2
#事务对象集合实现
transaction.contextClass=me.zkevin.transaction.context.DBTransactionContext
#netty资源数量
transaction.channelSize=5
transaction.bootstrapSize=2
#等待事务两阶段提交结果超时配置
transaction.callModel=sync
transaction.callTimeoutPerLoop=10
transaction.callTimeoutLoops=6000
#当前服务两阶段事务服务ip与端口
#针对多网卡，默认本机ip
transaction.ip=127.0.0.1
transaction.port=8910
#两阶段事务工作线程数量
transaction.executeWorkerSize=20